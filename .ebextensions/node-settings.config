container_commands:
    01_node_binary:
        command: "ln -sf `ls -td /opt/elasticbeanstalk/node-install/node-v16* | head -1`/bin/node /bin/node"
        leader_only: true
    02_migration:
        command: "node ace migration:run --force"
        leader_only: true
        ignoreErrors: true


option_settings:
  - option_name: NPM_CONFIG_PRODUCTION
    value: true


