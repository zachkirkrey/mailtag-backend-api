option_settings:

  - option_name: NPM_CONFIG_PRODUCTION
    value: true

  - option_name: node_binary
    command: "ln -sf `ls -td /opt/elasticbeanstalk/node-install/node-v16* | head -1`/bin/node /bin/node"

  - option_name: build-production
    command: node ace build --production

  - option_name: migration
    command: "node ace migration:run"

